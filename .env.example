# Firebase認証情報
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json

# Gemini API設定（オプション）
USE_GEMINI=0
GEMINI_API_KEY=
GEMINI_MODEL=gemini-1.5-flash
GEMINI_MAX_SAMPLE_ROWS=5
GEMINI_TIMEOUT_MS=12000

# CSV処理設定
ENCODING=utf8
DRY_RUN=0
LIMIT=
SAVE_MAPPING=0

echo '! .env.example' | sed 's/! /!/' >> .gitignore
git add .gitignore
git commit -m "Track .env.example"
git push
git check-ignore -v .env .env.example